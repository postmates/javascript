{
  "name": "@postmates/eslint-config",
  "version": "1.0.0",
  "description": "Postmates' ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "prelint": "editorconfig-tools check * rules/* test/*",
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "prepublish": "(in-install || eslint-find-rules --unused) && (not-in-publish || npm test)",
    "pretest": "npm run --silent lint",
    "test": "npm run --silent tests-only",
    "pretravis": "cd ../eslint-config-base && npm install && npm link && cd - && npm link --no-save @postmates/eslint-config-base",
    "travis": "npm run --silent test",
    "posttravis": "npm unlink @postmates/eslint-config-base >/dev/null &"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/postmates/javascript"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "postmates",
    "javascript",
    "styleguide"
  ],
  "author": "Kristen Mills",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/postmates/javascript/issues"
  },
  "homepage": "https://github.com/postmates/javascript",
  "dependencies": {
    "@postmates/eslint-config-base": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-airbnb": "^2.4.0",
    "babel-tape-runner": "^2.0.1",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^3.19.0 || ^4.3.0",
    "eslint-find-rules": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "in-publish": "^2.0.0",
    "react": ">= 0.13.0",
    "tape": "^4.7.0"
  },
  "peerDependencies": {
    "eslint": "^3.19.0 || ^4.3.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0"
  },
  "engines": {
    "node": ">= 4"
  }
}
