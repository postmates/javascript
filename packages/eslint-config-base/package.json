{
  "name": "@postmates/eslint-config-base",
  "version": "1.0.0",
  "description": "Postmates' base JS ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "prelint": "editorconfig-tools check * rules/* test/*",
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "prepublish": "(in-install || eslint-find-rules --unused) && (not-in-publish || npm test)",
    "pretest": "npm run --silent lint",
    "test": "npm run --silent tests-only",
    "pretravis": ":",
    "travis": "npm run --silent test",
    "posttravis": ":"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/postmates/javascript"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "postmates",
    "javascript",
    "styleguide"
  ],
  "author": "Kristen Mills",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/postmates/javascript/issues"
  },
  "homepage": "https://github.com/postmates/javascript",
  "devDependencies": {
    "babel-preset-airbnb": "^2.4.0",
    "babel-tape-runner": "^2.0.1",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^4.3.0",
    "eslint-find-rules": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "in-publish": "^2.0.0",
    "tape": "^4.7.0"
  },
  "peerDependencies": {
    "eslint": "^3.19.0 || ^4.3.0",
    "eslint-plugin-import": "^2.7.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "eslint-restricted-globals": "^0.1.1"
  }
}
